<!DOCTYPE html>
<html lang="{{locale}}">
  <head>
    <meta charset="utf-8">
    <meta name="description" content="{{{meta.description}}}" >
    <meta name="keywords" content="{{{meta.keywords}}}" >
    <link rel="icon" type="image/ico" href="/favicon.ico">
    <style>
        .firstload {
            position: fixed;
            left: 0;
            right: 0;
            top: 0;
            bottom: 0;
            z-index: 9999;
            background: #fff;
            display: block;
        }
        .firstload::after {
             content: '';
             background-image: url(data:image/svg+xml;base64,);
             background-repeat: no-repeat;
             height: 44px;
             width: 44px;
             position: absolute;
            left: 0;
            top: 0;
            right: 0;
            bottom: 0;
            margin: auto;
         }
    </style>
    {{#each assets.style}}
      <link rel="stylesheet" href="{{this}}">
    {{/each}}
    <title>{{{title}}}</title>
  </head>
  <body>
    {{{body}}}
    {{#each assets.script}}
      <script src="{{this}}"></script>
    {{/each}}
  </body>
</html>
